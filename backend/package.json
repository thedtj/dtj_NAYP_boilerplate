{
  "name": "graphql-yoga-prisma-backend-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate backend in nodejs with graphql-yoga, and prisma",
  "main": "index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "dev": "env-cmd ./config/dev.env nodemon src/index.js --ext js,graphql --exec babel-node",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "prisma deploy --env-file ./config/dev.env",
    "prisma-token": "prisma token -env-file ./config/dev.env"
  },
  "keywords": [
    "nodejs",
    "prisma",
    "graphql",
    "graphql-yoga",
    "boilerplate"
  ],
  "author": "Dan Todd <dantoddjr@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "dotenv": "^6.2.0",
    "env-cmd": "^8.0.2",
    "eslint-plugin-graphql": "^3.0.3",
    "graphql": "^14.1.1",
    "graphql-yoga": "^1.17.4",
    "prisma-binding": "^2.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.16.4"
  }
}
